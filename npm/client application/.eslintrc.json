{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    //"ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 2,
    "quotes": ["error", "double"],
    "import/extensions": [2, "always"],
    "prettier/prettier": "error"
  }
}
